<form method="post" id="frmEliminar" action="{{ path('clientes_delete', {'id': cliente.id}) }}" onsubmit="return confirm('Seguro que desea eliminar el cliente?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ cliente.id) }}">
</form>
