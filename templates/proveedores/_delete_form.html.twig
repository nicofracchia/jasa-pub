<form method="post" id="frmEliminar" action="{{ path('proveedores_delete', {'id': proveedore.id}) }}" onsubmit="return confirm('Seguro que desea eliminar este proveedor?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ proveedore.id) }}">
</form>
