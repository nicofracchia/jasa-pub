<form method="post" id="frmEliminar" action="{{ path('productos_delete', {'id': producto.id}) }}" onsubmit="return confirm('Seguro que desea eliminar este producto?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ producto.id) }}">
</form>
